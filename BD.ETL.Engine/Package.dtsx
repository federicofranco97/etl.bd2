            Row.NombreDia = Fecha.Date.DayOfWeek.ToString();
            Row.NombreMes = Fecha.ToString("MMM", System.Globalization.CultureInfo.InvariantCulture);
            Row.Trimestre = GetTrimestre(Fecha.Month);
            Row.Dia = Fecha.Day;
            Row.Mes = Fecha.Month;
            Row.Año = Fecha.Year;
        }
    }

